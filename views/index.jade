doctype html
html
  head
    meta(charset='utf-8')
  body
    
    label(for='email') 이메일
    input.js-email(type='text' name='email' minlength='5' size='20')
    br
    
    label(for='password') 비밀번호
    input.js-password(type='text' name='password' minlength='5' size='20')
    br
    
    label(for='name') 이름
    input.js-name(type='text' name='name' minlength='5' size='20')
    br
    
    label(for='birth-date') 생년월일
    input.js-birth-date(type='text' name='birth-date' minlength='5' size='20')
    br

    select.js-selector(name="selector")
      each q in titles
        option(value=q) #{q}
    
    br
    input.js-answer(type='text' name='answer' minlength='5' size='20')
    
    input.js-btn(type='button' value='회원가입')

    script(src='http://code.jquery.com/jquery-latest.min.js')
    script(src='/javascripts/index.js')
